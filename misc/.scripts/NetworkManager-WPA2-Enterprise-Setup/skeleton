[wifi-security]
key-mgmt=wpa-eap

[connection]
id=PLACE_NETWORK_ID_HERE
uuid=PLACE_UUID_HERE
type=wifi

[ipv6]
method=auto

[wifi]
ssid=PLACE_SSID_HERE
mode=infrastructure
security=802-11-wireless-security

[802-1x]
eap=peap
identity=PLACE_YOUR_ID_HERE
phase2-auth=mschapv2
password=PLACE_YOUR_PASSWORD_HERE

[ipv4]
method=auto
